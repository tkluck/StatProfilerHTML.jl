%table
  %caption Files sorted by exclusive sample count
  %thead
    %tr
      %th Exclusive
      %th Report
  %tbody
    - for file in $files
      %tr
        %td= $report.traces_by_file[file].exclusive |> fmtcount($report.tracecount)
        %td
          %a(href=outputfilename(file))= file
-# vim: set filetype=haml:
